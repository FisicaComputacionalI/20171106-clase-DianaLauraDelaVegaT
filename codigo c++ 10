#include <iostream>

using namespace std;

int main(){
char variable;
cout<<"Escribe una opcion entre A,B,C,D,F"<<endl;
cin>>variable;
variable=toupper(variable);
switch(variable){
  case 'A':cout<<"Excelente"<<endl; break;
  case 'B':cout<<"Muy Bien"<<endl; break;
  case 'C':cout<<"Bien"<<endl; break;
  case 'D':cout<<"Pasaste"<<endl; break;
  case 'F':cout<<"Intenta de nuevo el sig semestre"<<endl; break;
  default:cout<<"Opcion no valida"<<endl;
  }
  return 0;
}
